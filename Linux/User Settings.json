	"C_Cpp.default.intelliSenseMode": "clang-x64",
	"C_Cpp.errorSquiggles": "Disabled",
	"C_Cpp.intelliSenseEngine": "Default",
	"code-runner.clearPreviousOutput": false,
	"code-runner.executorMap": {
		"c": "cd $dir && gcc $fileName -o ${fileBasenameNoExtension}.out -lm && $dir${fileBasenameNoExtension}.out",
		"cpp": "cd $dir && g++ $fileName -o ${fileBasenameNoExtension}.out -lm && $dir${fileBasenameNoExtension}.out"
	},
	"code-runner.preserveFocus": true,
	"code-runner.runInTerminal": true,
	"code-runner.saveFileBeforeRun": true,
	"editor.acceptSuggestionOnEnter": "off",
	"editor.detectIndentation": false,
	"editor.fontFamily": "'SF Mono', 'PingFang SC', Consolas, 'Courier New', monospace",
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.insertSpaces": false,
	"editor.snippetSuggestions": "top",
	"explorer.confirmDelete": false,
	"files.autoGuessEncoding": true,
	"files.defaultLanguage": "cpp",
	"git.ignoreMissingGitWarning": true,
	"markdown.preview.fontFamily": "'PingFang SC', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
	"terminal.integrated.fontSize": 13,
	"window.menuBarVisibility": "toggle",
	"window.titleBarStyle": "custom",
	"window.zoomLevel": 0,
	"workbench.colorCustomizations": {
		"titleBar.activeBackground": "#21252B",
		"titleBar.inactiveBackground": "#282c34"
	},
	"workbench.colorTheme": "One Dark Pro",
	"workbench.statusBar.feedback.visible": false,
	"zenMode.centerLayout": false
